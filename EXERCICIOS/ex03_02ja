const prompt = require ('prompt-sync')()

let ano =  Number (prompt('Digite o ano atual'))
let anoNasc = Number (prompt('Digite o ano de nascimento'))
let subtracao = ano - anoNasc
if(subtracao < 10){console.log('CrianÃ§a')}
else if(subtracao == 11 || subtracao == 12 || subtracao == 13 || subtracao == 14 || subtracao == 15 || subtracao == 16 || subtracao== 17) {console.log('Adolescente')}
else if(subtracao > 60){console.log('Idoso')}
